{
  "name": "liste",
  "description": "todo list",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js:bundle.js -p 3000 --no-debug -- -b bundle-collapser/plugin -g unassertify -g yo-yoify -g uglifyify",
    "test": "standard && npm run test-snapshots && tap tests/*.js",
    "test-snapshots": "tap tests/components/*.js",
    "coverage": "tap --coverage-report=lcov tests/*.js"
  },
  "repository": "git://github.com/charlotteis/liste",
  "license": "AGPL-3.0",
  "dependencies": {
    "budo": "^10.0.3",
    "bundle-collapser": "^1.2.1",
    "choo": "^5.6.2",
    "uglifyify": "^4.0.1",
    "unassertify": "^2.0.4",
    "yo-yoify": "^3.7.3"
  },
  "engines": {
    "node": "8",
    "npm": "github:npm/npm#release-next"
  },
  "devDependencies": {
    "assert-snapshot": "^1.0.1",
    "choo-log": "^6.1.2",
    "standard": "^10.0.2",
    "tap": "^10.5.1"
  }
}
